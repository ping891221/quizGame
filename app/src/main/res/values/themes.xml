<resources>
    <!-- Base application theme. -->
    <style name="Theme.Quit" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="actionBarSize">100dip</item>
        <item name="colorPrimary">@color/mainOrange</item>
        <item name="colorPrimaryVariant">@color/navyBlue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/navyBlue</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.Quit.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


</resources>